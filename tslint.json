{
  "extends": ["tslint-config-airbnb", "gts/tslint.json"],
  "rules": {
    "no-any": false,
    "no-inferrable-types": false,
    "max-line-length": [true, 185],
    "ter-arrow-parens": true,
    "array-type": [false],
    "array-bracket-spacing": true,
    "only-arrow-functions": false,
    "ter-prefer-arrow-callback": false
  }
}
